<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cyduval.gps1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>gps1</name>
    <description>
      gps
    </description>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="customer/stryng/android/ldpi.png" />
    <icon density="mdpi" src="customer/stryng/android/mdpi.png" />
    <icon density="hdpi" src="customer/stryng/android/hdpi.png" />
    <icon density="xhdpi" src="customer/stryng/android/xhdpi.png" />
    <splash density="land-hdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="land-ldpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="land-mdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="land-xhdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="land-xxhdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="port-hdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="port-ldpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="port-mdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="port-xhdpi" src="customer/stryng/android/splash_screen.png" />
    <splash density="port-xxhdpi" src="customer/stryng/android/splash_screen.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
</widget>
